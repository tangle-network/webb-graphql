{
  "name": "webb-graphql",
  "version": "1.0.0",
  "description": "A collection of Subgraphs for Webb's protocols",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "dkg:deploy-dev": "cd packages/dkg && yarn deploy:dev",
    "lint": "nx run-many --all --target=lint",
    "lint:fix": "nx run-many --all --target=lint:fix",
    "format": "prettier --write \"**/*.{ts,js}\" ",
    "format:check": "prettier --check \"**/*.{ts,js}\" "
  },
  "devDependencies": {
    "@nx/eslint-plugin": "^16.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "nx": "^16.4.1",
    "prettier": "^2.8.8",
    "@polkadot/api": "^9.0.1",
    "@subql/cli": "latest",
    "@subql/types": "latest",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@webb-tools/tangle-substrate-types": "^0.3.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "ipfs-unixfs": "6.0.6"
  }
}
